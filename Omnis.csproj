  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Icons\MainTabControl\AppHub.png" />
    <Resource Include="Assets\Icons\MainTabControl\PasswordManager.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Assets\Icons\SVG\" />
