  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="9.0.3" />
    <PackageReference Include="Vanara.Windows.Shell" Version="4.1.1" />
  </ItemGroup>

</Project>
